# c-basic
 #include<stdio.h>
 #include<conio.h>
 void main()
 {
   char str1[12]="HELLO";
   char str2[12]="WORLD";
   char str3[12];
   int len;
   clrscr();
   strcpy(str3,str1);
   printf("strcpy(str3,str1):%s\n",str3);
   strcat(str1,str2);
   printf("strcat(str1,str2):%s\n",str1);
   len=strlen(str1);
   printf("strlen(str1):%d\n",len);
   getch();

}
